AiTalk
 {

	if RAND(100) < communicateratio
 	{
 		StartCommunicate
	}
 	else
	{
 		if mikireflag == 0
		{
			_result = AllTalk
			while ASEARCH(RE_REPLACE(_result,"((\\(_)?([0-9ac-vx-zA-Z?!])(\[[0-9a-zA-Z,\\\.\(\)\[\]'\-;:_!?'*]+\])*)|(\\w[0-9])|(\\b[0-9]))+",""),lastdialogues) != -1 && worried != 1
			{
				_result = AllTalk
			}
			if ARRAYSIZE(lastdialogues) >= 30
			{
				lastdialogues[0] = IARRAY
			}
			lastdialogues ,= RE_REPLACE(_result,"((\\(_)?([0-9ac-vx-zA-Z?!])(\[[0-9a-zA-Z,\\\.\(\)\[\]'\-;:_!?'*]+\])*)|(\\w[0-9])|(\\b[0-9]))+","")
			
			if girduty == 1
			{
				girduty = 0
				"%(gpose)%(geyes)%(gsmile)%(arms)\w5"
			}
			--
			_result

		}
 		else
		{ 
		MikireTalk
 		}
	}
 }
 
 
