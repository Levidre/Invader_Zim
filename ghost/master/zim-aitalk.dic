	{
		"\i[1]\w9\w9\w6\s[10]%(zeyes)\i[100]%(zsmile)%(arms)"
		disguised = 2
	}
	else
	{
		"\i[1]\w9\w9\w6\s[11]%(zeyes)\i[100]%(zsmile)%(arms)"
		disguised = 1
	}
	--
	"\1\i[1]\w9\w9%(gpose)%(gsatisfiedface)%(arms)\0"
}

OnGDisguise
{
	if disguised == 1
	{
		disguised = 0
	}
	elseif disguised == 2
	{
		"\i[1]\9\w9%(gpose)%(gsatisfiedface)%(arms)"
		"\i[1]\w9\w9\w6\s[11]%(zeyes)\i[100]%(zsmile)%(arms)"
		disguised = 1
	}
}

OnChoosePose
{
	if discovered == 1
	{
		if weekday == 6 || weekday == 0
		{
			if timeslot == "afternoon" || timeslot == "lunch" || timeslot == "morning"
			{
				_rand = RAND(3)
				if _rand == 1
				{
					disguised = 2
				}
				elseif _rand == 0
				{
					disguised = 0
				}
				else
				{
					disguised = 1
				}
			}
			elseif timeslot == "evening" || timeslot == "earlymorning"
			{
				if RAND(2) == 1
				{
					disguised = 2
				}
				else
				{
					disguised = 0
				}
			}
			else
			{
				disguised = 0
			}
		}
		else
		{
			if timeslot == "afternoon" || timeslot == "lunch" || timeslot == "morning"
			{
				disguised = 1
			}
			elseif timeslot == "evening" || timeslot == "earlymorning"
			{
				if RAND(2) == 1
				{
					disguised = 2
				}
				else
				{
					disguised = 0
				}
			}
			else
			{
				disguised = 0
			}
		}
	}
	else
	{
		disguised = 1
	}
}

